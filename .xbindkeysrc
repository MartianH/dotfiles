# Raise volume
"sh -c 'pactl set-sink-mute 0 false ; pactl set-sink-volume 0 +5%'"
    XF86AudioRaiseVolume

# Lower volume
"sh -c 'pactl set-sink-mute 0 false ; pactl set-sink-volume 0 -5%'"
    XF86AudioLowerVolume

# Mute volume
"pactl set-sink-mute 0 toggle"
    XF86AudioMute

# Brightness down
"light -U 2"
   XF86KbdBrightnessDown

# Brightness down
"light -A 2"
   XF86KbdBrightnessUp

