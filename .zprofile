#~/.zprofile

ANDROID_HOME=/home/martialh/Android/Sdk
PATH="$PATH:/home/martialh/.bin:/home/martialh/Android/Sdk/tools"
PANEL_FIFO="/tmp/panel_fifo"
XDG_CONFIG_HOME="$HOME/.config"
XDG_CACHE_HOME="$HOME/.cache"
XDG_DATA_HOME="$HOME/.local/share"
STEAM_RUNTIME=0
EDITOR=nvim
_JAVA_AWT_WM_NONREPARENTING=0

export ANDROID_HOME
export PATH
export PANEL_FIFO
export XDG_CONFIG_HOME
export XDG_CACHE_HOME
export XDG_DATA_HOME
export STEAM_RUNTIME
export EDITOR
export _JAVA_AWT_WM_NONREPARENTING
